# Build subdirectories.
/*build*
!/build-aux
!/build_msvc

*.pyc

# Only ignore unexpected patches
*.patch
!contrib/guix/patches/*.patch
!depends/patches/**/*.patch

/CMakeUserPresets.json

# Previous releases
/releases

# cargo default target dir
target/

/guix-build-*

/ci/scratch/

# --- Local build artifacts (do not commit) ---
/build/
/build-*/
/build-win/
/build-probe/
/stage/
/stage-*/
/stage-win/
/multiflex-*.zip
/multiflex-*.tar.gz
/SHA256SUMS

